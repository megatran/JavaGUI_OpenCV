package application;

import com.aldebaran.qi.Application;
import com.aldebaran.qi.Session;
//Aldebaran lib
import com.aldebaran.qi.helper.proxies.ALVideoDevice;

class videoResolution {
	//0 = kQQVGA, 1 = kQVGA, 2 = kVGA 
	public final int kQQVGA = 0;
	public final int kQVGA = 1;
	public final int kVGA = 2;
}
public class CameraModule {

	private ALVideoDevice camProxy;        
	
	public CameraModule(String[] robotIP) {
        // Create a new application
        Application application;
        application = new com.aldebaran.qi.Application(robotIP);
        // Start your application
        application.start();
        
		// Create a proxy to ALVideoDevice on the robot
		this.camProxy = new ALVideoDevice(application.session());
		
		// Then specify the resolution among : kQQVGA (160x120), kQVGA (320x240),
		// kVGA (640x480) or k4VGA (1280x960, only with the HD camera).		
	    // <param name="resolution"> Resolution requested. { 0 = kQQVGA, 1 = kQVGA, 2 = kVGA } </param>
	    // <param name="colorSpace"> Colorspace requested. { 0 = kYuv, 9 = kYUV422, 10 = kYUV, 11 = kRGB, 12 = kHSY, 13 = kBGR } </param>		
		
		String clientName = camProxy.subscribe("test", 1, 13, 30);
		

	}
