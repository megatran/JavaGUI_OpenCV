package application;

import com.aldebaran.qi.Application;
import com.aldebaran.qi.Session;
//Aldebaran lib
import com.aldebaran.qi.helper.proxies.ALVideoDevice;


public class CameraModule {

	private ALVideoDevice camProxy;        
	
	public CameraModule(String[] robotIP) {
        // Create a new application
        Application application;
        application = new com.aldebaran.qi.Application(robotIP);
        // Start your application
        application.start();
        
		// Create a proxy to ALVideoDevice on the robot
		this.camProxy = new ALVideoDevice(application.session());
		String clientName = camProxy.subscribe("test", kQVGA, kBGRColorSpace, 30)
		

	}
